{
  "editor.tabSize": 2,
  "editor.rulers": [120],
  "editor.formatOnSave": true,
  "mochaExplorer.autoload": false,
  "conventionalCommits.scopes": ["user", "company", "location module"],
  "editor.codeActionsOnSave": {
    "source.organizeImports": "always"
  },
  "workbench.colorCustomizations": {
    "activityBar.background": "#193131",
    "titleBar.activeBackground": "#234545",
    "titleBar.activeForeground": "#F8FCFC"
  },
  "dart.flutterSdkPath": null,
  "dart.checkForSdkUpdates": true,
  "dart.allowAnalytics": false,
  "dart.closingLabels": true,
  "dart.flutterCreateIOSLanguage": "swift",
  "dart.flutterCreateAndroidLanguage": "kotlin",
  "flutter.sdk": null,
  "files.associations": {
    "*.dart": "dart"
  },
  "emmet.includeLanguages": {
    "dart": "html"
  }
}
